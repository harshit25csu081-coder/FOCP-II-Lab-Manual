#include<iostream>
#include<vector>
using namespace std;
void boolisprime(int x){

    for(int i=2;i<=x/2;i++){
        if((x/2)%i==0)
            cout<<"not a prime";
            break;
        }


        cout<<"prime"; 
    }

        int main(){
            int x;
            cout<<"enter a value:";
            cin>>x;



            boolisprime(x);
            return 0;
        }