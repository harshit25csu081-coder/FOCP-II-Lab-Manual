QUESTION 9 

#include <iostream>
#include <cctype>
using namespace std;

int main() {
    char ch;
    cin >> ch;

    // Check if alphabet
    if(isalpha(ch)) {
        ch = tolower(ch);  
        
        if(ch=='a' || ch=='e' || ch=='i' || ch=='o' || ch=='u')
            cout << "Vowel";
        else
            cout << "Consonant";
    }


QUESTION 12
#include <iostream>
using namespace std;

int main() {
    int a, b, c;
    cin >> a >> b >> c;

    if(a == b && b == c)
        cout << "Equilateral";
    else if(a == b || b == c || a == c)
        cout << "Isosceles";
    else
        cout << "Scalene";

    return 0;
}
    
    else if(isdigit(ch)) {
        cout << "Number";
    }
    
    else {
        cout << "Special Character";
    }

    return 0;
}


QUESTION 11
#include <iostream>
using namespace std;

int main() {
    int items;
    float price, total;

    cin >> items >> price;

    total = items * price;

    if(items > 1000)
        total = total - (0.10 * total);

    cout << total;

    return 0;
}

QUESTION 10
#include <iostream>
using namespace std;

int main() {
    int year;
    cin >> year;

    if((year % 4 == 0 && year % 100 != 0) || (year % 400 == 0))
        cout << "Leap Year";
    else
        cout << "Not Leap Year";

    return 0;
}