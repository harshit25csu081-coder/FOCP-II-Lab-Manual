#include <iostream>
using namespace std;

int main() {
    int a, b, temp;

    cout << "Enter the value of a and b: ";
    cin >> a >> b;

    temp = a;
    a = b;
    b = temp;

    cout << "After swapping:" << endl;
    cout << "a = " << a << endl;
    cout << "b = " << b << endl;

    return 0;
}



question 7
#include <iostream>
using namespace std;

int main() {
    int a, b, c;
    cout << "Enter three scores: ";
    cin >> a >> b >> c;

    if(a > b && a > c)
        cout << "Player 1 is winner";
    else if(b > a && b > c)
        cout << "Player 2 is winner";
    else
        cout << "Player 3 is winner";

    return 0;
}




question 8
#include <iostream>
using namespace std;

int main() {
    int n;

    cout << "Enter value of n: ";
    cin >> n;

    for(int i = 1; i <= n; i++) {
        if(i % 3 == 0 && i % 5 == 0)
            cout << i << " Buzz Fuzz" << endl;
        else if(i % 3 == 0)
            cout << i << " Buzz" << endl;
        else if(i % 5 == 0)
            cout << i << " Fuzz" << endl;
        else
            cout << i << endl;
    }

    return 0;
}




question 6
#include <iostream>
using namespace std;

int main() {
    int n;
    float salary, bonus, netSalary;

    cout << "Enter number of employees: ";
    cin >> n;

    for (int i = 1; i <= n; i++) {
        cout << "Enter basic salary of employee " << i << ": ";
        cin >> salary;

        bonus = salary * 0.12;
        netSalary = salary + bonus;

        cout << "Bonus = " << bonus << endl;
        cout << "Net Salary = " << netSalary << endl;
    }

    return 0;
}