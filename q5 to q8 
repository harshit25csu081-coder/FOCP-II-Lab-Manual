#include <iostream>
using namespace std;

int main() {
    int a, b, temp;

    cout << "Enter the value of a and b: ";
    cin >> a >> b;

    temp = a;
    a = b;
    b = temp;

    cout << "After swapping:" << endl;
    cout << "a = " << a << endl;
    cout << "b = " << b << endl;

    return 0;
}